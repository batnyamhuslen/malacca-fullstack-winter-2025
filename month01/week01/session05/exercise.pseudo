//exercise 07
BEGIN 
  INPUT num 
  SET reversed = 0
  while num > 0 
    set digit =num MOD 10
    SET reversed = reversed *10+ digit
    SET num = num DIV 10
end while 
output reversed
end 
//exercise 08 
BEGIN
    INPUT n
    IF n <= 1 THEN
        OUTPUT "Анхны тоо биш"
    ELSE
        SET isPrime = TRUE
        FOR i = 2 TO n - 1
            IF n    i == 0 THEN
                SET isPrime = FALSE 
            END IF
        END FOR
        IF isPrime == TRUE THEN
            OUTPUT "Анхны тоо мөн"
        ELSE
            OUTPUT "Анхны тоо биш"
        END IF
    END IF
END
