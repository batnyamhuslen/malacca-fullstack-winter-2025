            IF n    i == 0 THEN
                SET isPrime = FALSE 
            END IF
        END FOR
        IF isPrime == TRUE THEN
            OUTPUT "Анхны тоо мөн"
        ELSE
            OUTPUT "Анхны тоо биш"
        END IF
    END IF
END
