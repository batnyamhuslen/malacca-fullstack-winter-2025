//exercise 10 
BEGIN 
  INPUT decimal
  SET binary = " "
  SET num = decimal
  WHILE num > 0 
     SET remainder = num  MOD 2 
     SET binary = remainder + binary
     SET num = num DIV 2 
end while
OUTPUT binary
end 
